{
  "name": "parcel-app",
  "private": true,
  "scripts": {
    "build-watch": "parcel watch ./src/client/index.html --no-hmr",
    "start-watch": "nodemon ./src/server/index.js",
    "dev": "concurrently --kill-others \"npm run start-watch\" \"npm run build-watch\"",
    "build": "parcel build ./src/client/index.html",
    "start": "npm run build && node server/index.js"
  },
  "devDependencies": {
    "@rbf/parcel-optimizer-javascript-obfuscator": "^0.2.2",
    "buffer": "^6.0.3",
    "concurrently": "^7.4.0",
    "eslint": "^8.23.1",
    "eslint-config-prettier": "^8.5.0",
    "nodemon": "^2.0.20",
    "parcel": "^2.0.0",
    "parcel-plugin-prettier": "^0.2.1",
    "parcel-reporter-static-files-copy": "^1.4.0",
    "postcss": "^8.4.16",
    "posthtml-include": "^1.7.4",
    "tailwindcss": "^3.1.8"
  },
  "dependencies": {
    "@mediapipe/pose": "^0.5.1635988162",
    "@tensorflow-models/pose-detection": "^2.0.0",
    "@tensorflow/tfjs-backend-wasm": "^3.20.0",
    "@tensorflow/tfjs-backend-webgl": "^3.20.0",
    "@tensorflow/tfjs-core": "^3.20.0",
    "aframe": "^1.3.0",
    "express": "^4.18.1",
    "howler": "^2.2.3",
    "jquery": "^3.6.1",
    "mind-ar": "^1.1.5",
    "scatter-gl": "^0.0.13",
    "three": "^0.144.0"
  }
}
